<UserControl x:Class="CustomNodeModel.CustomNodeModel.CustomComboBox"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:local="clr-namespace:CustomNodeModel.CustomNodeModel"
             mc:Ignorable="d"
             d:DesignHeight="50" d:DesignWidth="200">
    <Grid Margin="5">
        <ComboBox x:Name="CustomComboBox"
                  Width="150"
                  VerticalAlignment="Center"
                  IsEditable="False"
                  SelectedValue="{Binding SelectedValue, Mode=TwoWay}">
            <ComboBoxItem Content="All" />
            <ComboBoxItem Content="Option1" />
            <ComboBoxItem Content="Option2" />
            <ComboBoxItem Content="Option3" />
        </ComboBox>
    </Grid>
</UserControl>
